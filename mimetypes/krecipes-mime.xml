<?xml version="1.0" encoding="UTF-8"?>

<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">

	<mime-type type="application/x-krecipes-backup">
		<glob pattern="*.krecbk" />
		<sub-class-of type="application/x-gzip"/>
		<comment>Krecipes Backup file</comment>
		<comment xml:lang="cs">Záložní soubor Krecipes</comment>
		<comment xml:lang="de">Krecipes-Datensicherungsdatei</comment>
		<comment xml:lang="el">Krecipes εφεδρικό αρχείο</comment>
		<comment xml:lang="es">Archivo de copia de respaldo de Krecipes</comment>
		<comment xml:lang="nl">KRecipes reservekopiebestand</comment>
		<comment xml:lang="pt">Cópia de Segurança do Krecipes</comment>
		<comment xml:lang="pt_BR">Arquivo de backup do Krecipes</comment>
		<comment xml:lang="sk">Záložný súbor Krecipes</comment>
		<comment xml:lang="uk">файл резервної копії Krecipes</comment>
	</mime-type>

	<mime-type type="application/x-krecipes-recipes">
		<glob pattern="*.kreml" />
		<sub-class-of type="text/xml" />
		<comment>Krecipes Exported Recipes</comment>
		<comment xml:lang="cs">Exportované recepty Krecipes</comment>
		<comment xml:lang="de">Krecipes Exportierte Rezepte</comment>
		<comment xml:lang="el">Krecipes εξαχθείσες συνταγές</comment>
		<comment xml:lang="es">Recetas exportadas de Krecipes</comment>
		<comment xml:lang="nl">Geëxporteerde recepten van Krecipes</comment>
		<comment xml:lang="pt">Receitas Exportadas pelo Krecipes</comment>
		<comment xml:lang="pt_BR">Receitas exportadas do Krecipes</comment>
		<comment xml:lang="sk">Exportované recepty Krecipes</comment>
		<comment xml:lang="uk">експортовані рецепти Krecipes</comment>
	</mime-type>

	<mime-type type="application/x-krecipes-recipes-compressed">
		<glob pattern="*.kre" />
		<sub-class-of type="application/x-tarz"/>
		<comment>Krecipes Exported Recipes (compressed data)</comment>
		<comment xml:lang="cs">Exportované recepty Krecipes (zkomprimovaná data)</comment>
		<comment xml:lang="de">Krecipes Exportierte Rezepte (komprimierte Daten)</comment>
		<comment xml:lang="el">Krecipes εξαχθείσες συνταγές (συμπιεσμένα δεδομένα)</comment>
		<comment xml:lang="es">Recetas exportadas de Krecipes (datos comprimidos)</comment>
		<comment xml:lang="nl">Geëxporteerde recepten van Krecipes (gecomprimeerde gegevens)</comment>
		<comment xml:lang="pt">Receitas Exportadas pelo Krecipes (dados comprimidos)</comment>
		<comment xml:lang="pt_BR">Receitas exportadas do Krecipes (dados compactados)</comment>
		<comment xml:lang="sk">Exportované recepty Krecipes (komprimované dáta)</comment>
		<comment xml:lang="uk">експортовані рецепти Krecipes (стиснені дані)</comment>
	</mime-type>

</mime-info>
